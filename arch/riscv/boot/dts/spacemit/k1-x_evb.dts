	sy8810l: sy8810l@70 {
		compatible = "spacemit,sy8810l";
		reg = <0x70>;

		dcdc1-supply = <&dcdc_1>;

		regulators {
			compatible = "pmic,regulator,sy8810l";

			edcdc_1: EDCDC_REG1 {
				regulator-name = "edcdc1";
				regulator-min-microvolt = <600000>;
				regulator-max-microvolt = <1500000>;
				regulator-always-on;
			};
